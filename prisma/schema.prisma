// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"

}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Beatmap {
  id         Int     @id @default(autoincrement())
  artist     String
  title      String
  title_en   String?
  mapper     String
  length     String
  mode       String
  difficulty String
  ranked_at  String?
  beatmaps_json Json?
  bg_url     String?
  created_at DateTime @default(now())
}
